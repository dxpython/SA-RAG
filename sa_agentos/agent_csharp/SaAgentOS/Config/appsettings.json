{
  "DeepSeek": {
    "ApiKey": "YOUR-APIKEY",
    "BaseUrl": "https://api.deepseek.com",
    "Model": "deepseek-chat"
  },
  "SaRag": {
    "Endpoint": "http://localhost:8000/rag/query",
    "UseGraph": true,
    "UseMemory": true,
    "TopK": 6
  },
  "Memory": {
    "StoragePath": "./agent_memory.json",
    "UseSaRagMemory": true
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "SaAgentOS": "Information"
    }
  }
}